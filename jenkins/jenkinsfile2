pipeline {
    agent none
    stages {
        stage("Android") {
            agent {
                label "Android"
            }
            steps {
                script {
                    cleanNode()
                }
            }
        }
    }
}

def cleanNode() {
    sh '''
 set +x
 ls  /home/jenkins/workspace
  ls  /home/jenkins/workspace
       set -x
    '''
}
