pipeline {
    agent none
    stages {
        stage("Android") {
            agent {
                label "Android"
            }
            steps {
                script {
                    cleanNode()
                }
            }
        }
    }
}

def cleanNode() {
   
 sh 'set +x'
 sh 'ls  /home/jenkins/workspace'
sh 'find '/home/jenkins/workspace' -maxdepth 1 ! -name 'test_master' ! -name 'test_master@tmp' -type f -exec rm -v {} +'
  sh 'ls  /home/jenkins/workspace'
       sh 'set -x'
   
}
