pipeline {
    agent none
    stages {
        stage("Android") {
            agent {
                label "Android"
            }
            steps {
                script {
                    cleanNode()
                }
            }
        }
    }
}

def cleanNode() {
    sh 'set +x'
    sh 'mkdir  /home/jenkins/workspace/rs'
    sh 'mkdir  /home/jenkins/workspace/ef'
    sh 'mkdir  /home/jenkins/workspace/gh'
    sh 'mkdir  /home/jenkins/workspace/lo'
    sh 'ls  /home/jenkins/workspace'
    sh 'find /home/jenkins/workspace -mindepth 1 -maxdepth 1 -type d -not -name "test-vf_master" -exec rm -rf {} +'
    sh 'set -x'
}
