pipeline {
    agent none
    stages {
        stage("Android") {
            agent {
                label "Android"
            }
            steps {
                script {
                    cleanNode()
                }
            }
        }
    }
}

def cleanNode() {
   
 sh 'set +x'
 sh 'ls  /home/jenkins/workspace'
sh 'find  /home/jenkins/workspace -mindepth 1 -name four -prune -o -exec rm -rf {} + '
  sh 'ls  /home/jenkins/workspace'
       sh 'set -x'
   
}
